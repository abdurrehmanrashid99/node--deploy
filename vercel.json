{
    "builds": [
      {
        
        "src": "index.js",
        
        "use": "@vercel/node"
      },
      {
        
        "src": "build",
        
        "use": "@vercel/static"
      }
    ],
    
        "route" :[
    {
      "src": "/products",
      
      "dest": "index.js"
    }, 
    {
      "src": "/products/(.*)",
      
      "dest": "index.js"
    },  
    {
        
      "src": "/(.*)",
      
      "dest": "build/"
    }
  ]

  }